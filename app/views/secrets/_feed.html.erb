<%= render "shared/flash_messages"%>
<% @secret.each do |secret|%>

  <div class="container d-flex flex-column justify-content-center mt-2 border rounded bg-light">
    <div class="container border-bottom border-success">
      <h3 class="fs-2 fw-bold"><%= secret.title %></h3>
    </div>
    <div class="container">
      <p class="ps-2"><%= secret.body %></p>
    </div>
  </div>
<% end %>